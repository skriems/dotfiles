{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "npx",
      "disabled": false,
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "10000"
      }
    },
    "github": {
      "disabled": false,
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    }
  }
}
