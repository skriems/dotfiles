snippet testpage
	{page p_testing if panman.source in testsrc}
	{text}
	<<div class="alert alert-error">
	<p>Testing Page</p>
	</div>>
	<<div class="alert">
	<p>current monad: $monad</p>
	</div>>
	{end text}

	[testing] {single required=NONE} Test a different monad?
	<1>
	<2>

	{
	if testing:
		if testing == 1: monad.set(1)
		if testing == 2: monad.set(2)
	}
	{end page p_testing}

	{page p_testing2 if panman.source in testsrc}
	{text}
	<<div class="alert alert-error">
	<p>Testing Page</p>
	</div>>
	<<div class="alert">
	<p>current monad: $monad</p>
	</div>>
	{end text}
	{end page p_testing}

