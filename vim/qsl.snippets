snippet demog
	{page p_soz1 if not pdl.gender or not pdl.birthmonth or not pdl.birthyear or not pdl.sta or pdl.sta.last > months(12)}
	[birthmonth_update if not pdl.birthmonth] {pdl-update birthmonth}
	[birthyear_update if not pdl.birthyear] {pdl-update birthyear}
	[gender_update if not pdl.gender] {pdl-update gender}
	[sta_update if not pdl.sta or pdl.sta.last > months(12)] {pdl-update sta}
	{end page p_soz1}

snippet demogint
	{page p_socio}
	[q01] {open-int 1 12 rows=1 cols=2 prompt="" varlabel="Birthmonth"} Your birthmonth
	[q02] {open-int 1900 2010 rows=1 cols=4 prompt="" varlabel="Birthyear"} Your birthyear
	[q03] {single-colorpicker horizontal=1 columns=2 width=200 varlabel="Gender"} Are you...
	<1/'Male'> Male
	<2/'Female'> Female
	{end page p_socio}

	{page p_age}
	[agegrp if 0] Agegroup
	<1> 18-24
	<2> 25-34
	<3> 35-44
	<4> 45-55

	{
	temp a = date() - date(q02,q01,15)
	age_calced = a.days/365
	if age_calced >= 18 and age_calced <= 24: agegrp.set(1)
	elif age_calced >= 25 and age_calced <= 34: agegrp.set(2)
	elif age_calced >= 35 and age_calced <= 44: agegrp.set(3)
	elif age_calced >= 45 and age_calced <= 55: agegrp.set(4)
	}
	{end pagep_age}

snippet scout
	{page p_scout${1:1}}
	{exit status=screenout if ${2}}
	{end page p_scout$1}${3}

snippet pdlup
	{page p_${1:name} if not pdl.$1 or pdl.$1.last > months(12)}
	[$1_update] {pdl-update $1}
	{end page p_$1}

snippet occg
	{page p_${1:occg} if (not pdl.$1 or pdl.$1.last > months(12))}
	[q_$1,q_$1s] {pdl-update $1}
	{
	if q_$1s: pdl.$1s.set(str(q_$1s))
	}
	{end page p_$1}

snippet quota
