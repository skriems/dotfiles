snippet customerLogo
	<<script>
	var elem = $('.customer-logo')[0];
	if (elem === undefined) {
		$('.yg-logo').css("float", "left");
		$('<div />').addClass('customer-logo').insertAfter('.yg-logo');
		$('<div style="clear:both;">').insertAfter('.customer-logo');
	}
	$('.customer-logo').css({
		 "background-image":"url('https://surveyfiles.yougov.com/static/${1:PROJECT/FILE}')",
		 "width":"${2:170}px",
		 "height":"${3:80}px",
		 "float": "right",
		 "margin-bottom": "10px",
		 "border-radius": "5px",
		 "-webkit-box-shadow": "0px 5px 15px -10px rgba(0,0,0,0.6)",
		 "-moz-box-shadow": "0px 5px 15px -10px rgba(0,0,0,0.6)",
		 "box-shadow": "0px 5px 15px -10px rgba(0,0,0,0.6)",
	});
	</script>>${4}

snippet custominput
	<<script>
	$(page_state).on('after_content_loaded', function () {
		$('.custom_input').on('keyup', function () {
			var index = $('.custom_input').index(this);
			var negIndex = index - $('.custom_input').length;
			$('input:text[id*="response"]').eq(negIndex).val($('.custom_input').eq(index).val());
		});
	});
	</script>>

